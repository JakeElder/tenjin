{
  "name": "@mindfulstudio/tenjin-ui",
  "version": "0.5.0-beta.1",
  "description": "UI Library for Tenjin",
  "license": "UNLICENSED",
  "scripts": {
    "develop": "start-storybook -p 5010 --ci",
    "build": "build-storybook --output-dir build",
    "deploy": "npx vercel deploy ./build --no-clipboard --token=$VERCEL_TOKEN --prod",
    "format": "prettier . --write",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "cp package.json src/"
  },
  "author": "Jake Elder",
  "publishConfig": {
    "access": "public",
    "directory": "src"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@mindfulstudio/tenjin-tsconfig": "0.4.0",
    "@mindfulstudio/tenjin-types": "0.4.0",
    "@styled-system/css": "^5.1.5",
    "@styled-system/should-forward-prop": "^5.1.5",
    "emotion-theming": "^10.0.27",
    "reset-css": "^5.0.1",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^5.3.19",
    "@types/styled-system": "^5.1.10",
    "@types/styled-system__css": "^5.0.13",
    "@types/styled-system__should-forward-prop": "^5.1.1",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "prettier": "^2.0.5",
    "typescript": "^3.9.5"
  }
}
